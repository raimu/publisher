   <define name="e_Output">
      <element name="Ausgabe">
         <a:documentation>Experimentell. Dieser Befehl ist ähnlich zu ObjektAusgeben und dient derzeit nur dazu, Text auszugeben, der auf mehrere Seite umbrachen werden kann.</a:documentation>
         <optional>
            <attribute name="bereich">
               <a:documentation>Der Name des Platzierungsbereichs für den Text.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <ref name="e_Text"/>
         </optional>
      </element>
   </define>
   <define name="e_Paragraph">
      <element name="Absatz">
         <a:documentation>Erzeugt einen Absatz innerhalb eines Textblocks.</a:documentation>
         <optional>
            <attribute name="erlaubeumbruch">
               <a:documentation>(Experimentell) Liste der Zeichen, bei denen ein Umbruch möglich ist. Das Leerzeichen wird nicht automatisch als Umbruchbunkt erkannt, wenn das Attribut angegeben ist.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="farbe">
               <a:documentation>Farbe des Absatzes. Muss vorher mit DefiniereFarbe definiert worden sein.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="textformat">
               <a:documentation>Name des zu benutzenden Textformats. Wird kein Textformat angegeben, nimmt das System das Textformat text</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="schriftart">
               <a:documentation>Name der Schriftart. Voreinstellung ist »text« (mit kleinem t).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="sprache">
               <a:documentation>Name der Sprache für die Silbentrennung.</a:documentation>
               <choice>
                  <value>Deutsch</value>
                  <a:documentation xmlns="">Deutsche Trennmuster</a:documentation>
                  <value>Englisch (Großbritannien)</value>
                  <a:documentation xmlns="">Englische Trennmuster auswählen</a:documentation>
                  <value>Französisch</value>
                  <a:documentation xmlns="">Französische Trennmuster</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="class">
               <a:documentation>CSS Klasse für diesen Absatz.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="id">
               <a:documentation>CSS id für diesen Absatz.</a:documentation>
            </attribute>
         </optional>
         <zeroOrMore>
            <choice>
               <ref name="e_Action"/>
               <ref name="e_A"/>
               <ref name="e_I"/>
               <ref name="e_B"/>
               <ref name="e_Br"/>
               <ref name="e_Color"/>
               <ref name="e_HSpace"/>
               <ref name="e_U"/>
               <ref name="e_Fontface"/>
               <ref name="e_Image"/>
               <ref name="e_Sub"/>
               <ref name="e_Sup"/>
               <ref name="e_URL"/>
               <ref name="e_Value"/>
               <ref name="e_Switch"/>
               <ref name="e_Loop"/>
               <ref name="e_ForAll"/>
            </choice>
         </zeroOrMore>
      </element>
   </define>
   <define name="e_Pageformat">
      <element name="Seitenformat">
         <a:documentation>Legt die Größe des Ausgabemediums (z.B. DIN A4) fest. Voreinstellung ist eine Breite von 210mm und eine Höhe von 297mm.</a:documentation>
         <attribute name="breite">
            <a:documentation>Breite des Ausgabemediums.</a:documentation>
         </attribute>
         <attribute name="höhe">
            <a:documentation>Höhe des Ausgabemediums.</a:documentation>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="e_Pagetype">
      <element name="Seitentyp">
         <a:documentation>Definiert eine Seitenvorlage. Anhand der Kriterien, die im Attribut bedingung angegeben werden, wählt das System eine Seitenvorlage aus.</a:documentation>
         <attribute name="bedingung">
            <a:documentation>Wenn der XPath-Ausdruck »wahr« ergibt, dann wird diese Seite als Vorlage genommen.</a:documentation>
         </attribute>
         <attribute name="name">
            <a:documentation>Name der Seitenvorlage. Der Name ist zu Informationszwecken und zur Auswahl bei NeueSeite.</a:documentation>
         </attribute>
         <optional>
            <ref name="e_Margin"/>
         </optional>
         <optional>
            <oneOrMore>
               <ref name="e_PositioningArea"/>
            </oneOrMore>
         </optional>
         <interleave>
            <optional>
               <ref name="e_Grid"/>
            </optional>
            <optional>
               <ref name="e_AtPageCreation"/>
            </optional>
            <optional>
               <ref name="e_AtPageShipout"/>
            </optional>
         </interleave>
      </element>
   </define>
   <define name="e_PlaceObject">
      <element name="ObjektAusgeben">
         <a:documentation>Gibt ein rechteckiges Objekt (Bild, Tabelle, Box oder Textblock) aus.</a:documentation>
         <optional>
            <attribute name="belegen">
               <a:documentation>Bestimmt, ob im Raster der Platz für das Objekt als »belegt« markiert wird. Bei belegen=“nein” wird auch die aktuelle Zeile im Raster nicht verändert. Bei “auto” wird bei Bildern der
          Bildumriss beachtet, sofern eine Umrissdatei vorliegt. Siehe die Beschreibung bei Bild</a:documentation>
               <choice>
                  <value>ja</value>
                  <value>nein</value>
                  <text/>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="bereich">
               <a:documentation>Name des Bereichs (Platzierungsbereich), in dem das Objekt ausgegeben werden soll. Wird kein Bereich angegeben, so wird das Objekt auf der »Seite« (dem Bereich, der alle Rasterkästen
          beinhaltet) ausgegeben.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="gruppenname">
               <a:documentation>Wenn eine Gruppe ausgegeben werden soll, muss der Name hier angegeben werden.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="rahmen">
               <a:documentation>Es wird ein durchgezogener Rahmen um das Objekt gezeichnet. Zusätzlich muss die Rahmenfarbe angegeben werden. “ohne”: Es wird kein Rahmen gezeichnet.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="rahmenfarbe">
               <a:documentation>Die Farbe des Rahmens, wenn ein Rahmen ausgegeben wird. Nur sinnvoll zusammen mit dem Attribut rahmen.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="hintergrund">
               <a:documentation>Erzeugt einen farbigen Hintergrund für das Objekt. Das Attribut ›hintergrundfarbe‹ gibt die Farbe an.</a:documentation>
               <choice>
                  <value>vollständig</value>
                  <a:documentation xmlns="">Den Hintergrund in der angegebenen Farbe füllen (in hintergrundfarbe)</a:documentation>
                  <value>ohne</value>
                  <a:documentation xmlns="">Leerer Hintergrund</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="hintergrundfarbe">
               <a:documentation>Hintergrundfarbe wenn das Objekt mit einer Farbe hinterlegt werden soll.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="linienstärke">
               <a:documentation>Die Dicke des Rahmnes, der um das Objekt gezogen wird. Nur sinnvoll zusammen mit dem Attribut rahmen.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="spalte">
               <a:documentation>Zahl: Die Zeile im Raster, in der das Objekt ausgegeben wird. Wird die Zeile nicht angegeben, so sucht sich das System selbständig eine passende freie Zeile für das Objekt. Maßangabe:
          die Position von der oberen Blattkante.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="behalteposition">
               <a:documentation>Den virtuellen Cursor nicht verändern.</a:documentation>
               <choice>
                  <value>ja</value>
                  <value>nein</value>
                  <text/>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="seite">
               <a:documentation>Die Seite (später im PDF), auf der das Objekt erscheinen soll. Angabe als Zahl oder »next« für die nächste Seite.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="zeile">
               <a:documentation>Zahl: Die Zeile im Raster, in der das Objekt ausgegeben wird. Wird die Zeile nicht angegeben, so sucht sich das System selbständig eine passende freie Zeile für das Objekt. Maßangabe:
          die Position von der oberen Blattkante.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="maxhöhe">
               <a:documentation>Die maximale Höhe in Rasterzellen, die das Objekt belegen darf. Wird nur für den automatischen Tabellenumbruch benötigt. Die umbrochenen Tabellen auf den Folgeseiten haben maximal die
          angegebene Höhe.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="valign">
               <a:documentation>Wird ein Objekt ausgegeben, das kein vielfaches der Rasterhöhe ist, dann gibt das Attribut die vertikale Ausrichtung an. Bottom beispielsweise bedeutet, dass der freie Platz oberhalb des
          Objekts bleibt. Voreinstellung ist “top”.</a:documentation>
               <choice>
                  <value>top</value>
                  <a:documentation xmlns="">Das Objekt wird oben ausgerichtet.</a:documentation>
                  <value>middle</value>
                  <a:documentation xmlns="">Das Objekt wird so platziert, dass der Leerraum überhalb und unterhalb derselbe ist.</a:documentation>
                  <value>bottom</value>
                  <a:documentation xmlns="">Das Objekt wird unten ausgerichtet.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="hreferenz">
               <a:documentation>Bestimmt, ob die angegebene Spalte die linke oder die rechte Kante vom Objekt bezeichnet.</a:documentation>
               <choice>
                  <value>links</value>
                  <a:documentation xmlns="">Das Objekt wird in der angegebenen Spalte platziert.</a:documentation>
                  <value>rechts</value>
                  <a:documentation xmlns="">Das Objekt wird mit dem rechten Rand in der gegebenen Spalte ausgerichtet.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="vreferenz">
               <a:documentation>Bestimmt die Platzierung des Objekts relativ zur angegebenen Zeile.</a:documentation>
               <choice>
                  <value>bottom</value>
                  <a:documentation xmlns="">Die Zeile gibt die untere Kante des Objekts an.</a:documentation>
                  <value>top</value>
                  <a:documentation xmlns="">Die Zeile gibt die obere Kante des Objekts an (Voreinstellung).</a:documentation>
               </choice>
            </attribute>
         </optional>
         <oneOrMore>
            <interleave>
               <optional>
                  <ref name="e_Image"/>
               </optional>
               <optional>
                  <ref name="e_Box"/>
               </optional>
               <optional>
                  <ref name="e_Rule"/>
               </optional>
               <optional>
                  <ref name="e_Textblock"/>
               </optional>
               <optional>
                  <ref name="e_Barcode"/>
               </optional>
               <optional>
                  <ref name="e_Table"/>
               </optional>
            </interleave>
         </oneOrMore>
      </element>
   </define>
   <define name="e_PositioningArea">
      <element name="Platzierungsbereich">
         <a:documentation>Beschreibt einen Platzierungsbereich, der mehrere Platzierungsrahmen beinhalten kann. In diesen Rahmen können Elemente platziert werden</a:documentation>
         <attribute name="name">
            <a:documentation>Name des Platzierungsbereichs</a:documentation>
         </attribute>
         <oneOrMore>
            <ref name="e_PositioningFrame"/>
         </oneOrMore>
      </element>
   </define>
   <define name="e_PositioningFrame">
      <element name="Platzierungsrahmen">
         <a:documentation>Beschreibt einen rechteckigen Bereich, in dem Elemente platziert werden können. Inhalte in diesen Bereichen werden bei Platzmangel automatisch in den nächsten Rahmen deselben Bereichs
        platziert.</a:documentation>
         <attribute name="breite">
            <a:documentation>Breite des Rahmens in Rasterkästen</a:documentation>
         </attribute>
         <attribute name="höhe">
            <a:documentation>Höhe des Rahmens in Rasterkästen</a:documentation>
         </attribute>
         <attribute name="zeile">
            <a:documentation>Erste Zeile des Rahmens</a:documentation>
         </attribute>
         <attribute name="spalte">
            <a:documentation>Erste Spalte des Rahmens</a:documentation>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="e_ProcessNode">
      <element name="BearbeiteKnoten">
         <a:documentation>Bearbeitet alle angegebenen Datensätze. Die Elemente, für die die Regeln ausgeführt werden sollen, werden mit dem Attribut auswahl angegeben.</a:documentation>
         <attribute name="auswahl">
            <a:documentation>Auswahl der Kindelemente, die verarbeitet werden sollen.</a:documentation>
         </attribute>
         <optional>
            <attribute name="modus">
               <a:documentation>Name des Modus. Dieser muss bei Datensatz exakt übereinstimmen. Damit ist es möglich für dasselbe Element unterschiedliche Regeln aufzurufen.</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="e_Record">
      <element name="Datensatz">
         <a:documentation>Enthält die Anweisungen, die bei dem angegebenen Datensatz ausgeführt werden sollen. Der oberste Datensatz (d.h. direkt unterhalb des Wurzelelements) wird vom Publisher aufgerufen, alle
        nachfolgenden Datensätze müssen über das Element BearbeiteKnoten aufgerufen werden.</a:documentation>
         <attribute name="element">
            <a:documentation>Der Name des Elements der auf den Datensatz passt.</a:documentation>
         </attribute>
         <optional>
            <attribute name="modus">
               <a:documentation>Name des Modus der mit dem in BearbeiteKnoten übereinstimmt.</a:documentation>
            </attribute>
         </optional>
         <oneOrMore>
            <optional>
               <ref name="e_DefineColor"/>
            </optional>
            <optional>
               <ref name="e_EmptyLine"/>
            </optional>
            <optional>
               <ref name="e_ForAll"/>
            </optional>
            <optional>
               <ref name="e_Group"/>
            </optional>
            <optional>
               <ref name="e_Hyphenation"/>
            </optional>
            <optional>
               <ref name="e_InsertPages"/>
            </optional>
            <optional>
               <ref name="e_LoadDataset"/>
            </optional>
            <optional>
               <ref name="e_Loop"/>
            </optional>
            <optional>
               <ref name="e_Message"/>
            </optional>
            <optional>
               <ref name="e_NewPage"/>
            </optional>
            <optional>
               <ref name="e_NextRow"/>
            </optional>
            <optional>
               <ref name="e_NextFrame"/>
            </optional>
            <optional>
               <ref name="e_Output"/>
            </optional>
            <optional>
               <ref name="e_PlaceObject"/>
            </optional>
            <optional>
               <ref name="e_ProcessNode"/>
            </optional>
            <optional>
               <ref name="e_SaveDataset"/>
            </optional>
            <optional>
               <ref name="e_SavePages"/>
            </optional>
            <optional>
               <ref name="e_SetVariable"/>
            </optional>
            <optional>
               <ref name="e_SortSequence"/>
            </optional>
            <optional>
               <ref name="e_Switch"/>
            </optional>
            <optional>
               <ref name="e_Until"/>
            </optional>
            <optional>
               <ref name="e_While"/>
            </optional>
         </oneOrMore>
      </element>
   </define>
   <define name="e_Regular">
      <element name="Normal">
         <a:documentation>Dient als Festlegung des Schriftschnittes, wenn keine Formatierung angegeben ist.</a:documentation>
         <attribute name="schriftart">
            <a:documentation>Name der Schriftdatei, die geladen werden soll.</a:documentation>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="e_Rule">
      <element name="Linie">
         <a:documentation>Zeichnet eine horizontale oder vertikale Linie im Raster.</a:documentation>
         <attribute name="richtung">
            <a:documentation>Bestimmt die Ausrichtung der Linie.</a:documentation>
            <choice>
               <value>horizontal</value>
               <a:documentation xmlns="">Horizontale Linie</a:documentation>
               <value>vertikal</value>
               <a:documentation xmlns="">Vertikale Linie</a:documentation>
            </choice>
         </attribute>
         <optional>
            <attribute name="linienstärke">
               <a:documentation>Dicke der Linie in Rasterzellen oder als absolute Angabe.</a:documentation>
            </attribute>
         </optional>
         <attribute name="länge">
            <a:documentation>Länge der Linie in Rasterzellen oder als absolute Angabe.</a:documentation>
         </attribute>
         <optional>
            <attribute name="farbe">
               <a:documentation>Farbe der Linie. Vorgabe: Schwarz</a:documentation>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="e_SaveDataset">
      <element name="SpeichereDatensatzdatei">
         <a:documentation>Speichert eine XML-Datei auf Festplatte. Diese Datei muss als Kindelement hierarchische Struktur, zum Beispiel aus den Befehlen Element und Attribut enthalten, wahlweise zusammengesetzt
        durch Variablen. Eine ausführlichere Erläuterung findet sich im Abschnitt über automatisch generierte Verzeichnisse.</a:documentation>
         <attribute name="dateiname">
            <a:documentation>Name der Dateisatzdatei. Beispiel: toc</a:documentation>
         </attribute>
         <attribute name="elementname">
            <a:documentation>Name des Wurzelelements, das erzeugt wird.</a:documentation>
         </attribute>
         <optional>
            <attribute name="auswahl">
               <a:documentation>Alternativ zur Angabe des Datensatzes im Kindelement.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="attribute">
               <a:documentation>Der Variablenname (als XPath-Ausdruck, also $foo), der Attribut-Elemente beinhaltet. Diese Attribute werden zum Wurzelelement hinzugefügt.</a:documentation>
            </attribute>
         </optional>
         <oneOrMore>
            <optional>
               <ref name="e_Loop"/>
            </optional>
            <optional>
               <ref name="e_Element"/>
            </optional>
            <optional>
               <ref name="e_Makeindex"/>
            </optional>
            <optional>
               <ref name="e_SortSequence"/>
            </optional>
