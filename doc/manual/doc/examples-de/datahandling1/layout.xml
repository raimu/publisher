<?xml version="1.0" encoding="UTF-8"?>
<Layout xmlns="urn:speedata.de:2009/publisher/de">
  <Datensatz element="data">
    <ObjektAusgeben>
      <Tabelle breite="5" dehnen="max" padding="2pt">
        <Tr>
          <Td align="left"><Absatz><Wert>Artikelnummer</Wert></Absatz></Td>
          <Td align="right"><Absatz><Wert>Preis (Euro)</Wert></Absatz></Td>
        </Tr>
        <FürAlle auswahl="entry">
          <Tr>
            <Td align="left"><Absatz><Wert auswahl="@artikelnr"/></Absatz></Td>
            <Td align="right"><Absatz><Wert auswahl="@preis"/></Absatz></Td>
          </Tr>
        </FürAlle>
      </Tabelle>
    </ObjektAusgeben>
  </Datensatz>
</Layout>